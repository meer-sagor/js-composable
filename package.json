{
  "author": "Meer Sagor",
  "name": "@meersagor/js-composable",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "description": "JS-Composable is a lightweight utility library designed to provide practical helper methods that simplify real-world\nuse cases.",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "formData",
    "customFontFace",
    "base64",
    "js-utility"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/js-composable.cjs",
  "module": "./dist/js-composable.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/js-composable.js",
      "require": "./dist/js-composable.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "@changesets/cli": "^2.27.9"
  }
}
